{
  "name": "spotify-to-yaml-action",
  "version": "8.2.0",
  "description": "Export a seasonal Spotify playlist to YAML.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "eslint .",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/index.ts"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.4.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "@types/spotify-web-api-node": "^5.0.11",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.5.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "js-yaml": "^4.1.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/library-pals/spotify-to-yaml-action.git"
  },
  "bugs": {
    "url": "https://github.com/library-pals/spotify-to-yaml-action/issues"
  },
  "homepage": "https://github.com/library-pals/spotify-to-yaml-action#readme",
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!spotify-to-yaml)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
