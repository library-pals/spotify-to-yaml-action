{
  "name": "spotify-to-yaml-action",
  "version": "7.0.0",
  "description": "Export a seasonal Spotify playlist to YAML.",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/index.ts",
    "prepare": "husky install"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.18",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "js-yaml": "^4.1.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/spotify-to-yaml-action.git"
  },
  "bugs": {
    "url": "https://github.com/katydecorah/spotify-to-yaml-action/issues"
  },
  "homepage": "https://github.com/katydecorah/spotify-to-yaml-action#readme",
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!spotify-to-jekyll)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
