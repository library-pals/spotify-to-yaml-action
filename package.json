{
  "name": "spotify-to-yaml-action",
  "version": "8.1.0",
  "description": "Export a seasonal Spotify playlist to YAML.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/index.ts"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "js-yaml": "^4.1.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/spotify-to-yaml-action.git"
  },
  "bugs": {
    "url": "https://github.com/katydecorah/spotify-to-yaml-action/issues"
  },
  "homepage": "https://github.com/katydecorah/spotify-to-yaml-action#readme",
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!spotify-to-yaml)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
